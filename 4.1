def f():
    try:
        a = int(input("Введите число для проверки на делимость на 3: "))
    except ValueError:
        print('Введено не число!')
    else:
        if a % 3 == 0:
            print("Число ", a, " делится на 3")
        else:
            print("Число ", a, " не делится на 3")

f()

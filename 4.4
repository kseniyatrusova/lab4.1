def f():
    a = input("Введите номер билета: ")
    b = 0
    d = 0
    if len(a)%2==0:
        c = int(len(a)/2)
        for i in a[0:c]:
            b = b + int(i)
        for i in a[c:int(c*2)+1]:
            d = d + int(i)
        if b == d: print("Вааау, это счастливый билет =D")
        else: print("Эххх, этот билет несчастливый =( ")
    else: print("Нечётное количество цифр =( ")

f()
